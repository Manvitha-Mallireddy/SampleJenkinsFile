                steps{echo "build successful"}
            }
          stage("Deploy")
            {
                steps{echo "deploy successful"}
            }
           stage("Test")
            {
                steps{echo "test successful"}
            }
           stage("Release")
            {
                steps{echo "release successful"}
            }
      }
}
